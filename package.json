{
  "name": "slate-table",
  "version": "0.1.0",
  "description": "table plugin for slate.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist/*",
    "lint": "eslint . --fix",
    "start": "tsc -p . --watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlulic/slate-table.git"
  },
  "keywords": [
    "slate",
    "table",
    "html"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlulic/slate-table/issues"
  },
  "homepage": "https://github.com/nlulic/slate-table#readme",
  "peerDependencies": {
    "slate": ">=0.100.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.0",
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "eslint": "8.36.0",
    "eslint-plugin-import": "2.27.5",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "simple-git-hooks": "2.8.1",
    "slate-hyperscript": "0.77.0",
    "typescript": "5.0.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*.{css,md,js,jsx,json,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
